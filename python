import random
number = random.randint(0,20)
print("你一共有5次输入的机会，不仅技术，还要运气哟")
int1 = input("请猜一个0到20的数字：")
number = int(number)
guss = int(int1)
a = 1
while guss  != number and a <= 10:
    if guss > number:
        print("大了大了，小哥哥！")
        guss = input("再猜一猜呗：")
        guss = int(guss)
    else:
        print("小了小了，小哥哥！")
        guss = input("再猜一猜呗：")
        guss = int(guss)
        a +=1
else:
    print("哎哟，猜对了小机灵鬼，你是我肚子里的蛔虫吗")
    guss = int(guss)
print("Gamevoer！！")